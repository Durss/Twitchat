		"vue": "^3.3.9",
		"vue-country-flag-next": "^2.3.2",
		"vue-draggable-plus": "^0.6.1",
		"vue-facing-decorator": "^4.0.1",
		"vue-i18n": "9.14.5",
		"vue-router": "^4.6.4",
		"vue-select": "github:Durss/vue-select#beta",
		"vue-tippy": "^6.7.1",
		"vue-tsc": "^2.2.12",
		"vue3-carousel": "^0.15.1",
		"vue3-marquee": "^4.2.2",
		"vuedraggable": "^4.1.0"
	},
	"overrides": {
		"undici": "^6.23.0",
		"ajv": "^8.18.0",
		"minimatch": ">=10.2.1"
	}
}