{
	"name": "twitchat",
	"version": "16.13.8",
	"private": false,
	"author": "Durss",
	"description": "Full featured alternative Twitch chat",
	"type": "module",
	"scripts": {
		"dev": "concurrently --kill-others \"npm run front/watch\" \"npm run server/watch\"",
		"devall": "npm run server/start && npm run labels/start && concurrently --kill-others \"npm run front/watch\" \"npm run server/watch\"",
		"build": "npm run i18n && npm run lande && npm run front/build && npm run server/build",
		"tunnel": "cloudflared tunnel run dev",
		"front/watch": "vite --host",
		"front/watch-force": "vite --host --force",
		"front/build": "vue-tsc --noEmit && vite build",
		"server/watch": "tsc -w -p ./src_back/tsconfig.json",
		"server/build": "tsc -p ./src_back/tsconfig.json",
		"server/start": "pm2 start bootstrap-pm2.json",
		"labels/start": "pm2 start labels-pm2.json",
		"lande": "npx rollup --config lande.config.js",
		"patch": "npm run versionUpgrade patch",
		"minor": "npm run versionUpgrade minor",
		"major": "npm run versionUpgrade major",
		"i18n": "node src_labels/index.mjs",
		"front/typecheck": "vue-tsc --noEmit",
		"server/typecheck": "tsc --noEmit -p ./src_back/tsconfig.json",
		"lint": "eslint . --ext .vue,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"check-circular-dependencies": "dpdm ./src_front/main.ts",
		"versionUpgrade": "node scripts/versionSuffix.mjs",
		"analyse": "esbuild src_front/main.ts --bundle --analyze",
		"genapi": "node ./scripts/generatePublicApiDocumentation.mjs"
	},
	"dependencies": {
		"@fastify/cors": "^11.2.0",
		"@fastify/formbody": "^8.0.2",
		"@fastify/multipart": "^9.4.0",
		"@fastify/rate-limit": "^10.3.0",
		"@fastify/static": "^9.0.0",
		"ajv": "^8.18.0",
		"core-js": "^3.47.0",
		"discord-interactions": "^3.4.0",
		"discord.js": "^14.25.1",
		"fast-json-patch": "^3.1.1",
		"fastify": "^5.7.4",
		"fastify-raw-body": "^5.0.0",
		"fastify-sse-v2": "^4.2.1",
		"googleapis": "^170.0.0",
		"jsonwebtoken": "^9.0.3",
		"lru-cache": "^11.2.4",
		"mime-types": "^3.0.2",
		"node-fetch": "^3.3.2"
	},
	"devDependencies": {
		"@babel/plugin-proposal-decorators": "^7.28.6",
		"@babel/plugin-transform-class-properties": "^7.28.6",
		"@babel/preset-env": "^7.28.6",
		"@imengyu/vue3-context-menu": "^1.5.3",
		"@intlify/unplugin-vue-i18n": "6.0.8",
		"@popperjs/core": "^2.11.8",
		"@rollup/plugin-commonjs": "^26.0.3",
		"@rollup/plugin-node-resolve": "^15.3.1",
		"@rushstack/eslint-patch": "^1.15.0",
		"@sentry/integrations": "^7.99.0",
		"@sentry/vue": "^7.120.4",
		"@streamerbot/client": "^1.12.2",
		"@types/dom-screen-wake-lock": "^1.0.3",
		"@types/dompurify": "^3.2.0",
		"@types/gsap": "^3.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/matter-js": "^0.19.8",
		"@types/mime-types": "^3.0.1",
		"@types/node": "^22.19.7",
		"@types/three": "^0.158.3",
		"@types/tmi.js": "^1.8.6",
		"@typescript-eslint/eslint-plugin": "^8.38.0",
		"@typescript-eslint/parser": "^8.38.0",
		"@vitejs/plugin-vue": "^6.0.3",
		"@vue/eslint-config-typescript": "^14.7.0",
		"@vue/tsconfig": "^0.5.1",
		"concurrently": "^7.6.0",
		"dom-to-image-more": "^3.7.2",
		"dpdm": "^3.14.0",
		"eslint": "^10.0.0",
		"eslint-plugin-vue": "^10.7.0",
		"grapheme-splitter": "^1.0.4",
		"groq-sdk": "^0.9.1",
		"gsap": "^3.14.2",
		"isomorphic-dompurify": "^1.13.0",
		"jsonpath-plus": "^10.3.0",
