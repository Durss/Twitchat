{
	"script": "./server/bootstrap.js",
	"name": "Twitchat",
	"env": {},
	"merge_logs": true,
	"vizion": true,
	"autorestart": true,
	"instance_var": "NODE_APP_INSTANCE",
	"pmx": true,
	"detached": true,
	"automation": true,
	"treekill": true,
	"username": "root",
	"windowsHide": true,
	"kill_retry_time": 100,
	"write": true,
	"kill_timeout": 3000,
	"shutdown_with_message": true,
	"ignore_watch": [
		"node_modules",
		"static",
		"src_back",
		"src_labels",
		"userData",
		".git",
		"announcements.json",
		"patreon",
		"donors",
		"discord",
		"patreon",
		"beta"
	],
	"watch": [
		"./server"
	],
	"exec_mode": "fork"
}
